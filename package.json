    "license": "Apache-2.0",
    "author": "Karsten Schmidt <k+npm@thi.ng>",
    "scripts": {
        "clean": "rm -rf build/*.*",
        "build": "mkdir -p build && ts-node-esm src/index.ts",
        "build:all": "yarn build",
        "build:banners": "ts-node-esm src/banners.ts font/thing-regular-0.0.7.otf",
        "postprocess": "/Applications/FontForge.app/Contents/MacOS/FontForge -lang=py -script src/postprocess.py",
        "debug": "mkdir -p build && ts-node-esm src/debug-grid.ts"
    },
    "dependencies": {
        "@thi.ng/file-io": "^0.3.10",
        "@thi.ng/geom": "^3.4.10",
        "@thi.ng/transducers": "^8.3.13",
        "fast-glob": "^3.2.12",
        "opentype.js": "^1.1.0"
    },
    "devDependencies": {
        "@types/node": "^18.7.18",
        "@types/opentype.js": "0.7.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.8.3"
    },
    "font": {
        "familyName": "thi.ng",
        "styleName": "Regular",
        "designer": "Karsten Schmidt",
